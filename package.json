{
  "name": "pricing-engine",
  "version": "1.0.0",
  "description": "Fastify pricing-engine",
  "main": "index.js",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint src/ --fix",
    "clean": "rimraf dist/",
    "build": "npm run clean && babel ./ --out-dir dist/ --ignore ./node_modules,**/__tests__/*",
    "dev": "DISABLE_REQUEST_LOG=true nodemon --exec babel-node src/index",
    "start": "npm run build && DISABLE_REQUEST_LOG=true node dist/src/index",
    "swagger": "babel-node src/index docs",
    "precommit": "npm run lint && npm test",
    "test": "npm run test:unit",
    "test:dev": "jest --env node -c jest.config.unit.js --watch",
    "test:unit": "LOG_LEVEL=silent jest --env node -c jest.config.unit.js --coverage",
    "format": "prettier --write \"src/**/*.+(js|ts|json|yml|yaml|md)\"",
    "knex": "node -r dotenv/config ./node_modules/.bin/knex",
    "migrate:make": "npm run knex -- migrate:make --knexfile ./src/configuration/databaseConfiguration.js",
    "migrate": "npm run knex -- migrate:latest --knexfile ./src/configuration/databaseConfiguration.js",
    "seed:make": "npm run knex -- seed:make --knexfile ./src/configuration/databaseConfiguration.js",
    "seed": "npm run knex -- seed:run --knexfile ./src/configuration/databaseConfiguration.js",
    "rollback": "npm run knex -- migrate:rollback --knexfile ./src/configuration/databaseConfiguration.js",
    "migrate:last": "npm run knex -- migrate:down --knexfile ./src/configuration/databaseConfiguration.js"
  },
  "dependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/runtime": "7.11.2",
    "ajv": "6.12.5",
    "ajv-errors": "1.0.1",
    "ajv-keywords": "3.5.2",
    "arecibo": "2.0.0",
    "dotenv": "8.2.0",
    "escape-json-node": "2.0.0",
    "fastify": "^3.4.1",
    "fastify-helmet": "5.0.3",
    "fastify-oas": "3.0.3",
    "json-rules-engine": "^5.0.3",
    "knex": "^0.21.5",
    "make-promises-safe": "5.1.0",
    "pg": "^8.3.3",
    "pino": "6.6.1",
    "rimraf": "3.0.2",
    "uuid": "^8.3.0"
  },
  "engines": {
    "node": ">=12.*"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "devDependencies": {
    "@babel/node": "7.10.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "faker": "5.1.0",
    "jest": "26.4.2",
    "jest-date-mock": "1.0.8",
    "nodemon": "2.0.4",
    "prettier": "2.1.2"
  }
}
